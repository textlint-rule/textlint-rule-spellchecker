{
  "name": "textlint-rule-spellchecker",
  "version": "2.0.0",
  "description": "textlint rule to check spellings with native spellchecker (NSSpellChecker, Hunspell, or the Windows 8 Spell Check API)",
  "main": "lib/spellchecker.js",
  "scripts": {
    "build": "babel src -d lib --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "npm-run-all lint mocha",
    "lint": "eslint src test",
    "mocha": "mocha"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "textlint",
    "rule",
    "english",
    "spellchecker"
  ],
  "author": "nodaguti",
  "license": "MIT",
  "bugs": "https://github.com/nodaguti/textlint-rule-spellchecker/issues",
  "homepage": "https://github.com/nodaguti/textlint-rule-spellchecker",
  "repository": "nodaguti/textlint-rule-spellchecker",
  "dependencies": {
    "spellchecker": "^3.3.1",
    "textlint-rule-helper": "^2.0.0",
    "textlint-util-to-string": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-plugin-espower": "^2.2.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.1.2",
    "power-assert": "^1.4.1",
    "textlint": "^7.0.1",
    "textlint-tester": "^2.0.0"
  }
}
